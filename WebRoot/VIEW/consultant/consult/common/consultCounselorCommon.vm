#set($counselorVO=$consultBase.counselorVO)
<div class="row">
	<div class="col-sm-12">
		<div class="panel panel-default">
		  <div class="panel-heading"><span class="glyphicon glyphicon-pushpin"></span>咨询师信息</div>
		  <div class="panel-body">
                  	<div class="col-sm-2">
                        <img src="$link.contextPath/UPLOAD/$!counselorVO.counselor.photo" class="img-rounded" style="width: 82px;height: 82px;">
                    </div>
                    <div class="col-sm-5">
	                    <p class="col-sm-12"><strong class=" text-right col-sm-5">名称:</strong>$!counselorVO.counselor.name</p>
	                    <p class="col-sm-12"><strong class=" text-right col-sm-5">注册邮箱:</strong>$!counselorVO.counselor.email</p>
	                    <p class="col-sm-12"><strong class=" text-right col-sm-5">公司信息:</strong>$!counselorVO.company.name</p>
                    </div>
                    <div class="col-sm-5">
	                    <p class="col-sm-12"><strong class=" text-right col-sm-5">手机号码</strong>$!counselorVO.counselor.mobile</p>
	                    <p class="col-sm-12"><strong class=" text-right col-sm-5">部门信息</strong>$!counselorVO.counselor.department</p>
                    </div>
                </div>	
  		</div>
	</div>
</div>
